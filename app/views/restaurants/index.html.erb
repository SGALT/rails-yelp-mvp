<div class="container">
  <%= link_to "create new restaurant", new_restaurant_path, class: 'btn btn-info' %>
  <ul>
    <% @restaurants.each do |restaurant| %>
      <h3>
        <%=link_to restaurant.name, restaurant_path(restaurant) %>
      </h3>
      <li>
        <%= restaurant.category %>
      </li>
    <% end %>
  </ul>
</div>
