<div class="container">
  <%= simple_form_for @restaurant do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.input :category, :collection => %w[chinese italian japanese french belgian] %>
    <%= f.button :submit %>
  <% end %>
  <%= link_to "back to restaurants list", restaurants_path, class: 'btn btn-primary' %>
</div>

