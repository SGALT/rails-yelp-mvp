<div class="container">
  <%= simple_form_for [@restaurant, @review] do |f| %>
    <%= f.input :content %>
    <%= f.input :rating, :collection => (0..5) %>
    <%= f.button :submit %>
  <% end %>
  <%= link_to "back to restaurants view", restaurant_path(@restaurant), class: 'btn btn-primary' %>
</div>
